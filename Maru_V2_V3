    if game.Players.LocalPlayer.Team == nil then
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetTeam", "Pirates")
    end
end)

print("Debug: Step2")

_G.RefundStat = false
_G.CheckList = {
    ['MAX_LEVEL'] = true,
    ['GOT_GODHUMAN'] = false,
    ['GOT_LEVER'] = false,
    ['GOT_CDK'] = false,
    ['GOT_CDK_350'] = false,
    ['GOT_MIR+VH'] = false,
}

